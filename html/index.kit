                <li class="minus"><i class="fas fa-minus"></i><span>brak opcji w pakiecie</span></li>
              </ul>
            </div>
          </div>
          <div class="btn">
            <button>Wybierz</button>
          </div>
        </div>
      </div>
      <div class="container-price">
        <div class="cards-area">
          <div class="row">
            <div class="pricing-details">
              <h3>Standard</h3>
              <p>$99</p>
            </div>
            <div class="offers-box">
              <ul class="features">
