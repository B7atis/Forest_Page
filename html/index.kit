  <link rel="stylesheet" href="dist/css/main.min.css">
</head>

<body>
  <!-- Navigation -->
  <nav class="nav wrapper">
    <div class="logo">
      <a href="#"><i class="fa-solid fa-tree"></i> Forest</a>
